<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.hting.mapper.oracle.dic.WinfoBigMapper">


    <update id="updateVersionCode">
        update WINFO_BIG
        set VERSIONCODE = #{versionCode}
        where bid = #{bid}
    </update>
    <update id="updateName">
        update WINFO_BIG
        set name = #{name}
        where bid = #{bid}

    </update>
</mapper>